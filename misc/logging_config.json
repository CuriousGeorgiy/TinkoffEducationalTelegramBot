{
	"version": 1,
	"handlers": {
		"fileHandler": {
			"class": "logging.FileHandler",
			"formatter": "standard_formatter",
			"filename": "misc/application.log"
		}
	},
	"loggers": {
		"main": {
			"handlers": ["fileHandler"],
			"level": "INFO"
		}
	},
	"formatters": {
		"standard_formatter": {
			"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
		}
	}
}
